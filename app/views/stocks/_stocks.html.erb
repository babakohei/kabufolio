<ul class="media-list">
  <% stocks.each do |stock| %>
    <li class="media">
      <div class="media-left">
        <img class="media-object img-rounded" src="<%= gravatar_url(micropost.user, { size: 50 }) %>" alt="">
      </div>
      <div class="media-body">
        <div>
          <%= link_to stock.user.name, user_path(stock.user) %> <span class="text-muted">posted_at <%= stock.created_at %></span>
        </div>
        <div>
          <p><%= stock.code %><%= stock.name %></p>
        </div>
      </div>
    </li>
  <% end %>
  <%= paginate stocks %>
</ul>